<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.auth.RegisterActivity"
    android:background="#000000">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="29dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="241dp"
        android:layout_marginBottom="628dp"
        android:text="회원가입"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="19dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="110dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="631dp"
        android:text="필수 정보"
        android:textColor="#969696"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="191dp"
        android:layout_marginEnd="281dp"
        android:layout_marginBottom="550dp"
        android:text="닉네임"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_nickname"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="509dp"
        android:backgroundTint="#707070"
        android:hint="2자 이상"
        android:textColor="#FFFFFF"
        android:textColorHint="#969696"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_validate"
        android:layout_width="76dp"
        android:layout_height="27dp"
        android:textColor="#3B3B3B"
        android:textSize="14sp"
        android:text="중복 확인"
        android:background="@drawable/button_validate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_nickname"
        />

    <TextView
        android:id="@+id/tv_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="287dp"
        android:layout_marginEnd="281dp"
        android:layout_marginBottom="454dp"
        android:text="이메일"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <EditText
        android:id="@+id/et_email"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="346dp"
        android:layout_marginBottom="413dp"
        android:backgroundTint="#707070"
        android:hint="이메일 입력"
        android:importantForAutofill="no"
        android:textColor="#FFFFFF"
        android:textColorHint="#969696"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="383dp"
        android:layout_marginEnd="269dp"
        android:layout_marginBottom="358dp"
        android:text="비밀번호"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_password"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="442dp"
        android:layout_marginBottom="318dp"
        android:backgroundTint="#707070"
        android:hint="6자 이상"
        android:textColor="#FFFFFF"
        android:textColorHint="#969696"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.472" />

    <Button
        android:id="@+id/btn_end_register1"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:background="#202425"
        android:text="다음"
        android:textColor="#575757"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/cb_admit_all"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="482dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="262dp"
        android:buttonTint="#FFFFFF"
        android:text="전체 약관 동의"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        android:onClick="onCheckboxClicked"/>


    <CheckBox
        android:id="@+id/cb_essential_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="523dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="221dp"
        android:buttonTint="#FFFFFF"
        android:text="필수    만 14세 이상 동의"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        android:onClick="onCheckboxClicked"/>


    <CheckBox
        android:id="@+id/cb_essential_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="196dp"
        android:buttonTint="#FFFFFF"
        android:text="필수    서비스 이용약관 동의"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        android:onClick="onCheckboxClicked"/>


    <CheckBox
        android:id="@+id/cb_essential_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="573dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="171dp"
        android:buttonTint="#FFFFFF"
        android:text="필수    개인정보 취급방식 동의"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        android:onClick="onCheckboxClicked"/>


    <CheckBox
        android:id="@+id/cb_choice_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="596dp"
        android:layout_marginEnd="308dp"
        android:layout_marginBottom="146dp"
        android:buttonTint="#FFFFFF"
        android:text="선택    마케팅 정보 수신 동의"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355"
        android:onClick="onCheckboxClicked"/>


</androidx.constraintlayout.widget.ConstraintLayout>